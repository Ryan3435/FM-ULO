<PUBLIC:COMPONENT>
<PUBLIC:ATTACH EVENT="onmouseover" ONEVENT="m_over()"/>
<PUBLIC:ATTACH EVENT="onmouseout" ONEVENT="m_out()"/>
<SCRIPT LANGUAGE="JScript">
var c_CSS;
function m_over()
{	c_CSS=className;
	className+="Hover";
	src="../images/ICON_view_over.gif";
}
function m_out()
{	className=c_CSS;
    src="../images/btn_add_contact.gif";
}
</SCRIPT>
</PUBLIC:COMPONENT>