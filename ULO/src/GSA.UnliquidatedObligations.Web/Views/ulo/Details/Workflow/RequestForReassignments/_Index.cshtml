@model GSA.UnliquidatedObligations.Web.Models.WorkflowViewModel

@{

    if (Model.Workflow.RequestForReassignments.Count == 0)
    {
        Html.RenderAction("Details", "RequestForReassignments", new {workflowId = Model.Workflow.WorkflowId});
    }
    else
    {
        Html.RenderAction("Details", "RequestForReassignments", new { id = Model.Workflow.RequestForReassignments.FirstOrDefault().RequestForReassignmentID, workflowId = Model.Workflow.WorkflowId });
    }

}